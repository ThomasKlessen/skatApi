    "benchmark": "node ./benchmarks/benchmark.js",
    "doc": "node ./docs.js"
  },
  "dependencies": {
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-status-monitor": "^1.2.5",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.0",
    "rotating-file-stream": "^1.4.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg-monitor": "^1.1.0",
    "pg-promise": "^8.6.5"
  },
  "devDependencies": {
    "typescript": "^3.4.5",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.12",
    "@types/morgan": "^1.7.35",
    "autocannon": "^3.2.1",
    "db-migrate": "^0.11.5",
    "db-migrate-pg": "^0.5.0",
    "jest": "^24.7.1",
    "jsdoc-generator": "^0.3.3",
    "nodemon": "^1.18.11",
    "ts-jest": "^24.0.2",
    "ts-node-dev": "^1.0.0-pre.39"
